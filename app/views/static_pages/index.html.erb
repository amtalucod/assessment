<h1> Index </h1>
<table class="table table-hover table-bordered">
    <thead class="table-success">
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>User ID</th>
        <th>Admin?</th>
        
        
      </tr>
    </thead>
    
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.name %></td>
          <td><%= link_to user.email, user_profile_path(user.id) %></td>
          <td><%= user.id %></td>
          <td><%= user.admin? %></td>
      <% end %>
    </tbody>
</table>

<%= link_to "New user", '#', class:"btn btn-success" %>



